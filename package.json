  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig.webpackCfg.json\" webpack --mode production",
    "clean": "rimraf dist",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "cross-env TS_NODE_PROJECT=\"tsconfig.webpackCfg.json\" webpack serve",
    "lint": "eslint 'src/**/*.{j,t}s{,x}'",
    "postinstall": "if command -v husky &> /dev/null; then husky install; fi",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --pretty --noEmit"
  },
